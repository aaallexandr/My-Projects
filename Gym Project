     },
     "user_tz": -120
    },
    "id": "A_Im_2uxM-Kq",
    "outputId": "de4ee4d3-6af3-46e1-8607-3c9026dab46c"
   },
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 1008x432 with 2 Axes>"
