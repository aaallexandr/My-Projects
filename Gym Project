    "# Зададим списки для цикла\n",
    "scores = ['user_score', 'critic_score']\n",
    "platforms = ['PS4', 'XOne']\n",
    "\n",
    "# Зададим цикл\n",
    "for p in platforms:\n",
    "    platform_actual_data = actual_data.loc[actual_data['platform'] == p]\n",
    "    for s in scores:\n",
    "        # Выведем корреляцию\n",
    "        display (f'Платформа {p}: корреляция между продажами и показателем {s}:', platform_actual_data['total_sales'].corr(platform_actual_data[s]))\n",
