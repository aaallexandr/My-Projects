    "        plt.figure(figsize=(8, 4))\n",
    "        sns.scatterplot(x=platform_actual_data['total_sales'], y=platform_actual_data[s], data=platform_actual_data, alpha=0.5)\n",
    "        plt.title(f'Платформа {p} — диаграммма рассеяния для продаж и {s}')\n",
    "        plt.xlabel('Продажи, млн. копий')\n",
    "        plt.ylabel('Оценка пользователя')\n",
    "        plt.grid(True)\n",
